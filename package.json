{
  "name": "hyphe-browser",
  "version": "0.1.0",
  "description": "Hyphe Browser",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test/tools/setup.js test",
    "watch:test": "npm test -- --watch --growl",
    "lint": "eslint .",
    "watch:lint": "watch-exec -c 'npm run lint' -w . -n -e node_modules,release,dist",
    "watch-all": "npm-run-all --parallel watch:*",
    "dev": "npm-run-all --parallel watch:electron watch:dev-server",
    "start": "cross-env NODE_ENV=production electron .",
    "release": "node bin/package.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "watch:electron": "cross-env NODE_ENV=development electron .",
    "watch:dev-server": "cross-env NODE_ENV=development node dev-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medialab/hyphe-browser.git"
  },
  "author": "ByteClub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/medialab/hyphe-browser/issues"
  },
  "homepage": "https://github.com/medialab/hyphe-browser#readme",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.36.2",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.14.0",
    "express": "^4.13.3",
    "jsdom": "^7.2.2",
    "json-loader": "^0.5.4",
    "mocha": "^2.3.4",
    "npm-run-all": "^1.4.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "tiny-promisify": "^0.1.0",
    "watch-exec": "^1.2.0",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-target-electron-renderer": "^0.3.0"
  },
  "dependencies": {
    "@naholyr/chromium-net-errors": "^1.0.4",
    "classnames": "^2.2.3",
    "electron-debug": "^0.5.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-intl": "^2.0.0-beta-2",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc4",
    "react-split-pane": "^0.1.22",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-localstorage": "^0.4.0",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.3",
    "uuid": "^2.0.1"
  }
}
