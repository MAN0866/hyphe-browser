{
  "name": "hyphe-browser",
  "version": "0.1.0",
  "description": "Hyphe Browser",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test/tools/setup.js test",
    "watch:test": "npm test -- --watch --growl",
    "lint": "eslint .",
    "watch:lint": "watch-exec -c 'npm run lint' -w . -n -e node_modules,release,dist",
    "watch-all": "npm-run-all --parallel watch:*",
    "dev": "npm-run-all --parallel watch:electron watch:dev-server",
    "start": "cross-env NODE_ENV=production electron .",
    "release": "cross-env NODE_ENV=production node bin/package.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "watch:electron": "cross-env NODE_ENV=development electron .",
    "watch:dev-server": "cross-env NODE_ENV=development node dev-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medialab/hyphe-browser.git"
  },
  "author": "ByteClub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/medialab/hyphe-browser/issues"
  },
  "homepage": "https://github.com/medialab/hyphe-browser#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.7",
    "eslint": "^2.0.0",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "jsdom": "^8.0.2",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.5.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "tiny-promisify": "^0.1.0",
    "url-loader": "^0.5.7",
    "watch-exec": "^1.2.2",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "@naholyr/chromium-net-errors": "^1.0.4",
    "classnames": "^2.2.3",
    "electron-debug": "^0.5.1",
    "electron-shortcut": "^0.4.1",
    "lodash.mapvalues": "^4.2.0",
    "react": "^0.14.7",
    "react-autosuggest": "^3.5.0",
    "react-dom": "^0.14.7",
    "react-intl": "^2.0.0-beta-2",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^3.0.0",
    "react-split-pane": "^0.1.23",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.4",
    "redux-localstorage": "^0.4.0",
    "redux-thunk": "^1.0.3",
    "tether": "^1.1.1",
    "uuid": "^2.0.1"
  }
}
